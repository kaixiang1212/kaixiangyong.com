    "@sveltejs/kit": "1.27.2",
    "@types/cookie": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
